{
  "version": "0.1.0-beta",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "clear_jest": "jest --clearCache",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "moola.js",
  "author": "Nelson M",
  "module": "dist/moola.js.esm.js",
  "size-limit": [
    {
      "path": "dist/moola.js.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/moola.js.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.4",
    "husky": "^9.1.4",
    "size-limit": "^11.1.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.3",
    "typescript": "^3.9.10"
  },
  "dependencies": {
    "decimal.js": "^10.4.3"
  },
  "publishConfig": {
    "@mwinel:registry": "https://npm.pkg.github.com"
  }
}
